<table class="table table-hover table-bordered table-margin">
  <thead>
    <tr>
      <th>Id</th>
      <th>Coluna</th>
      <th>Coluna</th>
      <th>Coluna</th>
      <th>Coluna</th>
      <th class="table-action"></th>
    </tr>
  </thead>

  <tbody>
    <% 3.times.each do |index|%>
    <tr>
      <td><%= index+1%></td>
      <td>Texto 1</td>
      <td>Texto 2</td>
      <td>Texto 3</td>
      <td>Texto 4</td>
      <td>
        <a href="#" class="glyphicon glyphicon-eye-open table-icon"><span></span></a>
      </td>
    </tr>
    <% end %>
  </tbody>
</table>

<div class="cb">
  <ul class="pagination">
    <li class="disabled"><a href="#">&laquo;</a></li>
    <% 5.times.each do |page|%>
    <li class="<%= page == 0 ? "active" : "" %>"><a href="#"><%= page+1%> <span class="sr-only">(current)</span></a></li>
    <% end %>
    <li ><a href="#">&raquo;</a></li>
  </ul>
</div>